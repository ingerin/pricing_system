{
  "builds": [
    {
      "src": "vercel_wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/vercel/path0",
    "DJANGO_SETTINGS_MODULE": "config.vercel_settings"
  },
  "buildCommand": "bash vercel_build.sh",
  "outputDirectory": "public"
}